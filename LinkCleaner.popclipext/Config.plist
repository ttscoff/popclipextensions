<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Image File</key>
			<string>icon.png</string>
			<key>Script Interpreter</key>
			<string>/usr/bin/ruby</string>
			<key>Shell Script File</key>
			<string>script.rb</string>
			<key>Title</key>
			<string>LinkCleaner</string>
			<key>Regular Expression</key>
			<string>(?si)(.*)?((http|https):\/\/)?[\w\-_]+(\.[\w\-_]+)+([\w\-\.,@?^=%&amp;:/~\+#\(\)_]*[\w\-\@^=%&amp;/~\+#])?(.*)?</string>
			<key>After</key>
			<string>paste-result</string>
		</dict>
	</array>
	<key>Credits</key>
	<array>
		<key>Name</key>
		<string>Brett Terpstra</string>
		<key>Link</key>
		<string>http://brettterpstra.com</string>
	</array>
	<key>Extension Description</key>
	<string>Lengthens and cleans urls in selection.</string>
	<key>Extension Identifier</key>
	<string>com.brettterpstra.popclip.extension.linkcleaner</string>
	<key>Extension Name</key>
	<string>LinkCleaner</string>
	<key>Version</key>
	<integer>2</integer>
</dict>
</plist>
